{
  "name": "caleydo_data_sql",
  "description": "Data provider plugin for loading tabular data stored in an SQLite database.",
  "version": "0.0.1",
  "license" : "SEE LICENSE IN LICENSE",
  "repository": "Caleydo/caleydo_data_sql",
  "dependencies": {
  },
  "peerDependencies": {
    "caleydo_server": "*"
  },
  "caleydo": {
    "type": "plugin",
    "category": "python_data",
    "plugins": {
      "python": [
        {
          "type": "dataset-provider",
          "id": "dataset-sql",
          "file": "sql"
        },
        {
          "type": "sql-adapter",
          "id": "sqlite",
          "file": "sqlite",
          "filter": "(sqlite3|sqlite)"
        }
      ]
    },
    "dependencies": {
      "python": {
        "SQLAlchemy" :"==1.0.12",
        "sqlalchemy-gevent": "==0.2"
      }
    }
  },
  "publishConfig": {
    "registry": "http://registry.caleydo.org/"
  }
}
